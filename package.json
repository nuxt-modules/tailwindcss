{
  "name": "@nuxtjs/tailwindcss",
  "version": "6.10.1",
  "description": "TailwindCSS module for Nuxt",
  "repository": "nuxt-modules/tailwindcss",
  "license": "MIT",
  "type": "module",
  "configKey": "tailwindcss",
  "compatibility": {
    "nuxt": "^2.9.0 || ^3.0.0-rc.1"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nuxi dev playground",
    "play": "pnpm dev",
    "dev:build": "nuxi build playground",
    "dev:generate": "nuxi generate playground",
    "dev:nuxt2": "nuxi dev nuxt2-playground",
    "dev:prepare": "nuxt-module-build prepare && nuxt-module-build build --stub && nuxi prepare playground && nuxi prepare docs",
    "postinstall": "pnpm dev:prepare",
    "build": "nuxt-module-build build",
    "prepack": "pnpm build",
    "release": "pnpm lint && pnpm test && pnpm prepack && pnpm changelogen --release --push && pnpm publish",
    "docs:build": "nuxi generate docs",
    "docs:dev": "nuxi dev docs",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "pnpm dev:prepare && tsc --noEmit && nuxi typecheck playground"
  },
  "dependencies": {
    "@nuxt/kit": "^3.8.2",
    "autoprefixer": "^10.4.16",
    "chokidar": "^3.5.3",
    "clear-module": "^4.1.2",
    "colorette": "^2.0.20",
    "consola": "^3.2.3",
    "defu": "^6.1.3",
    "h3": "^1.9.0",
    "micromatch": "^4.0.5",
    "pathe": "^1.1.1",
    "postcss": "^8.4.31",
    "postcss-custom-properties": "^13.3.2",
    "postcss-nesting": "^12.0.1",
    "tailwind-config-viewer": "^1.7.3",
    "tailwindcss": "~3.3.5",
    "ufo": "^1.3.2"
  },
  "devDependencies": {
    "@fontsource/inter": "^5.0.15",
    "@nuxt/content": "^2.9.0",
    "@nuxt/devtools": "^1.0.3",
    "@nuxt/eslint-config": "latest",
    "@nuxt/module-builder": "^0.5.4",
    "@nuxt/test-utils": "^3.8.1",
    "@tailwindcss/typography": "^0.5.10",
    "@types/micromatch": "^4.0.6",
    "changelogen": "^0.5.5",
    "destr": "^2.0.2",
    "eslint": "latest",
    "happy-dom": "^12.10.3",
    "jsdom": "^22.1.0",
    "nuxt": "^3.8.2",
    "typescript": "^5.2.2",
    "vitest": "0.33.0",
    "vue-tsc": "^1.8.22"
  },
  "packageManager": "pnpm@8.9.2",
  "resolutions": {
    "@nuxtjs/tailwindcss": "link:."
  }
}