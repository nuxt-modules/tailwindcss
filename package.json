{
  "name": "@nuxtjs/tailwindcss",
  "version": "6.8.0",
  "description": "TailwindCSS module for Nuxt",
  "repository": "nuxt-modules/tailwindcss",
  "license": "MIT",
  "configKey": "tailwindcss",
  "compatibility": {
    "nuxt": "^2.9.0 || ^3.0.0-rc.1",
    "bridge": false
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "dev": "nuxt dev playground",
    "dev:nuxt2": "nuxt dev nuxt2-playground",
    "lint": "eslint .",
    "prepack": "nuxt-module-build",
    "release": "pnpm test && pnpm changelogen --release --push && pnpm publish",
    "test": "vitest run"
  },
  "dependencies": {
    "@nuxt/kit": "^3.6.5",
    "@nuxt/postcss8": "^1.1.3",
    "autoprefixer": "^10.4.15",
    "chokidar": "^3.5.3",
    "clear-module": "^4.1.2",
    "colorette": "^2.0.20",
    "cookie-es": "^1.0.0",
    "defu": "^6.1.2",
    "destr": "^2.0.1",
    "h3": "^1.8.0",
    "iron-webcrypto": "^0.8.0",
    "micromatch": "^4.0.5",
    "pathe": "^1.1.1",
    "postcss": "^8.4.28",
    "postcss-custom-properties": "^13.3.0",
    "postcss-nesting": "^11.3.0",
    "radix3": "^1.1.0",
    "tailwind-config-viewer": "^1.7.2",
    "tailwindcss": "~3.3.3",
    "ufo": "^1.2.0",
    "uncrypto": "^0.1.3"
  },
  "devDependencies": {
    "@fontsource/inter": "^5.0.8",
    "@nuxt/content": "^2.7.2",
    "@nuxt/devtools": "^0.8.0",
    "@nuxt/eslint-config": "latest",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/test-utils": "^3.6.5",
    "@tailwindcss/typography": "^0.5.9",
    "@types/micromatch": "^4.0.2",
    "changelogen": "^0.5.4",
    "codecov": "latest",
    "eslint": "latest",
    "jsdom": "^22.1.0",
    "nuxt": "^3.6.5",
    "vitest": "^0.34.2"
  }
}
