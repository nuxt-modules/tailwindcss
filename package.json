{
  "name": "@nuxtjs/tailwindcss",
  "version": "6.12.0",
  "description": "TailwindCSS module for Nuxt",
  "repository": "nuxt-modules/tailwindcss",
  "license": "MIT",
  "type": "module",
  "configKey": "tailwindcss",
  "compatibility": {
    "nuxt": "^2.9.0 || ^3.0.0-rc.1"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "play": "pnpm dev",
    "prepare": "nuxt-module-build prepare",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:generate": "nuxi generate playground",
    "dev:preview": "nuxi preview playground",
    "dev:nuxt2": "nuxi dev nuxt2-playground",
    "dev:prepare": "pnpm prepare && pnpm build:stub",
    "build": "nuxt-module-build build",
    "build:stub": "pnpm build --stub",
    "prepack": "pnpm build",
    "release": "pnpm lint && pnpm test && pnpm prepack && pnpm changelogen --release --push && pnpm publish",
    "docs:build": "nuxi generate docs",
    "docs:preview": "nuxi preview docs",
    "docs:dev": "nuxi dev docs",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "pnpm dev:prepare && tsc --noEmit && nuxi typecheck playground"
  },
  "dependencies": {
    "@nuxt/kit": "^3.12.2",
    "autoprefixer": "^10.4.19",
    "consola": "^3.2.3",
    "defu": "^6.1.4",
    "h3": "^1.12.0",
    "pathe": "^1.1.2",
    "postcss": "^8.4.38",
    "postcss-nesting": "^12.1.5",
    "tailwind-config-viewer": "^2.0.4",
    "tailwindcss": "~3.4.4",
    "ufo": "^1.5.3",
    "unctx": "^2.3.1"
  },
  "devDependencies": {
    "@fontsource/inter": "^5.0.18",
    "@nuxt/content": "^2.13.0",
    "@nuxt/devtools": "^1.3.6",
    "@nuxt/eslint-config": "0.3.13",
    "@nuxt/module-builder": "^0.8.0",
    "@nuxt/test-utils": "^3.13.1",
    "@tailwindcss/typography": "^0.5.13",
    "changelogen": "^0.5.5",
    "destr": "^2.0.3",
    "eslint": "9.5.0",
    "happy-dom": "^13.10.1",
    "nuxt": "^3.12.2",
    "typescript": "^5.4.5",
    "vitest": "1.5.0",
    "vue-tsc": "^2.0.22"
  },
  "packageManager": "pnpm@9.4.0",
  "resolutions": {
    "@nuxtjs/tailwindcss": "link:."
  },
  "build": {
    "entries": [
      "src/runtime/merger.mjs"
    ],
    "externals": [
      "runtime/merger.mjs"
    ]
  },
  "stackblitz": {
    "startCommand": "pnpm dev:prepare && pnpm dev"
  }
}
